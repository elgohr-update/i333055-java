{
   "$schema": "https://docs.renovatebot.com/renovate-schema.json",
   "extends": ["local>i333055/renovate-config:base(maintance)"],
   "baseBranches": ["master"],
   "includePaths": [
      "client-java-contrib/**",
      "fluent-gen/**",
      "fluent/**"
   ],
   "packageRules": [
    {
      "matchPackagePatterns": ["*"],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "groupName": "all minor path dependencies",
      "groupSlug": "all-minor-patch",
      "commitMessageSuffix": " (all-minor-patch)",
      "prPriority": 6
    },
    {
       "matchPaths": [
          "client-java-contrib/**"
       ],
       "additionalBranchPrefix": "client-java-contrib/",
       "commitMessagePrefix": "client-java-contrib : "
     },
     {
       "matchPaths": [
          "fluent-gen/**"
       ],
       "additionalBranchPrefix": "fluent-gen/",
       "commitMessagePrefix": "fluent-gen : "
     },
     {
       "matchPaths": [
          "fluent/**"
       ],
       "additionalBranchPrefix": "fluent/",
       "commitMessagePrefix": "fluent : "
     }
   ]
}
