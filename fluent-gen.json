{
   "$schema": "https://docs.renovatebot.com/renovate-schema.json",
   "extends": ["local>i333055/renovate-config:base(fluent-gen)"],
   "baseBranches": ["master"],
   "separateMinorPatch": "true",
    "includePaths":[
      "fluent-gen/**"
    ],
   "packageRules": [
